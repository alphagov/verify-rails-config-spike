
<%= form_for :transaction, url: transactions_path do |f| %>
    <p>
      <%= f.label :entity_id %><br>
      <%= f.text_field :entity_id, value: 'http://www.test-rp.gov.uk/SAML2/MD' %>
    </p>

    <p>
      <%= f.label :matching_service_entity_id %><br>
      <%= f.text_field :matching_service_entity_id, value: 'http://www.test-rp-ms.gov.uk/SAML2/MD' %>
    </p>

    <p>
      <%= f.label :simple_id %><br>
      <%= f.text_area :simple_id, value: 'test-rp' %>
    </p>

    <p>
      <%= f.label :levels_of_assurance %><br>
      <%= f.text_area :levels_of_assurance, value: 'LEVEL_2' %>
    </p>

    <p>
      <%= f.label :assertion_consumer_service_uri %><br>
      <%= f.text_area :assertion_consumer_service_uri, value: 'http://localhost:50130/test-rp/login' %>
    </p>

    <p>
      <%= f.label :service_homepage %><br>
      <%= f.text_area :service_homepage, value: 'http://localhost:50130/test-rp' %>
    </p>

    <p>
      <%= f.label :signing_cert %><br>
      <%= f.text_area :signing_cert, value: '' %>
    </p>

    <p>
      <%= f.label :eidas_enabled %><br>
      <%= f.check_box :eidas_enabled, checked: false %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>